{
  "name": "the-roadcode-quiz",
  "version": "1.7.2",
  "repository": "git@github.com:alanrsoares/the-roadcode-quiz.git",
  "description": "NZ's Road Code Driving Theory Practice Quiz",
  "keywords": [],
  "main": "src/index.tsx",
  "homepage": "https://roadcodequiz.netlify.app/",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "lint": "eslint src",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "prebuild": "yarn generate-build-version && yarn write-app-name-to-manifest",
    "generate-build-version": "node scripts/generate-build-version",
    "write-app-name-to-manifest": "node scripts/write-app-name-to-manifest",
    "sync": "scripts/sync-db.sh",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "no-unused-vars": "error",
      "no-console": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "^18.3.1",
    "styled-components": "^5.3.11",
    "twin.macro": "^2.8.2",
    "twind": "^0.16.19",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/helper-define-map": "^7.18.6",
    "@babel/helper-regex": "^7.10.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.52",
    "@types/ramda": "^0.27.66",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "aa-db": "^2.1.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "gh-pages": "^3.2.3",
    "husky": "5.1.2",
    "invariant": "^2.2.4",
    "levenary": "^1.1.1",
    "lint-staged": "^10.5.4",
    "pinst": "^2.1.6",
    "prettier": "^2.8.8",
    "react-is": "^17.0.2",
    "react-scripts": "^5.0.1",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.5.4",
    "typescript-styled-plugin": "^0.18.3"
  },
  "license": "MIT"
}
